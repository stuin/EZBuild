cd=./
files=*.cpp
output=a.out
terminal=xterm
num=3

caching=s/.cpp/.o/g ; s: [^ ]*/: ../obj/:g
depfinder=s:#include \":../obj/:pg

builder1=g++ -c %files -std=c++17 -Wall -fpermissive
buildargs1=""

builder2=mv *.o ../obj/
buildargs2=""

builder3=g++ %cached -o %output -pthread
buildargs3=""

tester=%output
testargs=""
testdir=./
